<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:background="#393C49"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >

    <androidx.core.widget.NestedScrollView
        android:fillViewport="true"
        android:id="@+id/leftLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rightLayout"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <com.google.android.material.card.MaterialCardView
                app:cardBackgroundColor="@color/white"
                android:id="@+id/bottomCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                app:cardUseCompatPadding="true"
                app:contentPadding="20dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_weight="5">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/mostGrahpOrderLvl"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Chart Orders"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/dropdown_menuTwo"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/dropdown_menuTwo" />

                    <com.google.android.material.button.MaterialButton
                        android:textAllCaps="false"
                        app:icon="@drawable/arrow_down"
                        app:iconSize="14dp"
                        app:iconGravity="end"
                        app:iconPadding="10dp"
                        android:id="@+id/dropdown_menuTwo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:text="Today"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:strokeColor="@color/purple_200"
                        app:strokeWidth="1dp" />

                    <View
                        android:id="@+id/divider5"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="16dp"
                        android:background="?android:attr/listDivider"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/mostGrahpOrderLvl"
                        app:layout_constraintTop_toBottomOf="@+id/dropdown_menuTwo" />

                    <com.github.mikephil.charting.charts.PieChart
                        android:id="@+id/pieChart"
                        android:layout_width="0dp"
                        android:layout_height="300dp"
                        android:layout_margin="16dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/dropdown_menuTwo"
                        app:layout_constraintStart_toStartOf="@+id/mostGrahpOrderLvl"
                        app:layout_constraintTop_toBottomOf="@+id/divider5" />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/bottomCard2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="20dp"
                app:cardUseCompatPadding="true"
                app:contentPadding="20dp"
                app:layout_constraintTop_toBottomOf="@+id/bottomCard"
                app:layout_constraintVertical_weight="5">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/mostGrahpOrderLvl2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Most Favorites Items"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/dropdown_menuTwo2"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/dropdown_menuTwo2" />

                    <com.google.android.material.button.MaterialButton
                        android:textAllCaps="false"
                        app:icon="@drawable/arrow_down"
                        app:iconSize="14dp"
                        app:iconGravity="end"
                        app:iconPadding="10dp"
                        android:id="@+id/dropdown_menuTwo2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:text="Today"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:strokeColor="@color/purple_200"
                        app:strokeWidth="1dp" />

                    <View
                        android:id="@+id/divider52"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="16dp"
                        android:background="?android:attr/listDivider"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/mostGrahpOrderLvl2"
                        app:layout_constraintTop_toBottomOf="@+id/dropdown_menuTwo2" />

                    <com.github.mikephil.charting.charts.CombinedChart
                        android:id="@+id/pieChart2"
                        android:layout_width="0dp"
                        android:layout_height="300dp"
                        android:layout_margin="16dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/dropdown_menuTwo2"
                        app:layout_constraintStart_toStartOf="@+id/mostGrahpOrderLvl2"
                        app:layout_constraintTop_toBottomOf="@+id/divider52" />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

    <androidx.core.widget.NestedScrollView
        android:fillViewport="true"
        android:id="@+id/rightLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_weight="5"
        app:layout_constraintStart_toEndOf="@+id/leftLayout"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <com.google.android.material.card.MaterialCardView
                app:cardBackgroundColor="@color/white"
                android:id="@+id/rightLayoutbottomCard"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="20dp"
                app:cardUseCompatPadding="true"
                app:contentPadding="20dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_weight="5">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/rightLayoutmostGrahpOrderLvl"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Trending Items"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/rightLayoutdropdown_menuTwo"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/rightLayoutdropdown_menuTwo" />

                    <com.google.android.material.button.MaterialButton
                        android:textAllCaps="false"
                        app:icon="@drawable/arrow_down"
                        app:iconSize="14dp"
                        app:iconGravity="end"
                        app:iconPadding="10dp"
                        android:id="@+id/rightLayoutdropdown_menuTwo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:text="Today"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:strokeColor="@color/purple_200"
                        app:strokeWidth="1dp" />

                    <View
                        android:id="@+id/rightLayoutdivider5"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="16dp"
                        android:background="?android:attr/listDivider"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/rightLayoutmostGrahpOrderLvl"
                        app:layout_constraintTop_toBottomOf="@+id/rightLayoutdropdown_menuTwo" />

                    <com.github.mikephil.charting.charts.HorizontalBarChart
                        android:id="@+id/rightLayoutpieChart"
                        android:layout_width="0dp"
                        android:layout_height="300dp"
                        android:layout_margin="16dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/rightLayoutdropdown_menuTwo"
                        app:layout_constraintStart_toStartOf="@+id/rightLayoutmostGrahpOrderLvl"
                        app:layout_constraintTop_toBottomOf="@+id/rightLayoutdivider5" />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/rightLayoutbottomCard2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="20dp"
                app:cardUseCompatPadding="true"
                app:contentPadding="20dp"
                app:layout_constraintTop_toBottomOf="@+id/rightLayoutbottomCard"
                app:layout_constraintVertical_weight="5">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/rightLayoutmostGrahpOrderLvl2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Most Selling Items"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="@+id/rightLayoutdropdown_menuTwo2"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/rightLayoutdropdown_menuTwo2" />

                    <com.google.android.material.button.MaterialButton
                        android:textAllCaps="false"
                        app:icon="@drawable/arrow_down"
                        app:iconSize="14dp"
                        app:iconGravity="end"
                        app:iconPadding="10dp"
                        android:id="@+id/rightLayoutdropdown_menuTwo2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="16dp"
                        android:text="Today"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:strokeColor="@color/purple_200"
                        app:strokeWidth="1dp" />

                    <View
                        android:id="@+id/rightLayoutdivider52"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="16dp"
                        android:background="?android:attr/listDivider"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="@+id/rightLayoutmostGrahpOrderLvl2"
                        app:layout_constraintTop_toBottomOf="@+id/rightLayoutdropdown_menuTwo2" />

                    <com.github.mikephil.charting.charts.BarChart
                        android:id="@+id/rightLayoutpieChart2"
                        android:layout_width="0dp"
                        android:layout_height="300dp"
                        android:layout_margin="16dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="@+id/rightLayoutdropdown_menuTwo2"
                        app:layout_constraintStart_toStartOf="@+id/rightLayoutmostGrahpOrderLvl2"
                        app:layout_constraintTop_toBottomOf="@+id/rightLayoutdivider52" />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.google.android.material.card.MaterialCardView>


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.core.widget.NestedScrollView>




</androidx.constraintlayout.widget.ConstraintLayout>